--EJERCICIO TAREA

CREATE TABLE USUARIO_REG(ID INTEGER, EMAIL VARCHAR2(120), EDAD INTEGER);

CREATE OR REPLACE TRIGGER disparador_usuario_reg BEFORE INSERT ON usuario_reg FOR EACH ROW
BEGIN
IF TO_CHAR(SYSDATE,'HH24')<8 AND TO_CHAR(SYSDATE,'HH24')>=10 THEN
RAISE_APPLICATION_ERROR(-20001,'LO SIENTO NO ESTAS DENTRO DEL RANGO DE HORARIO PERMITIDO');
END IF;
END;
/
INSERT INTO USUARIO_REG VALUES(1,'XXX',20);
INSERT INTO USUARIO_REG VALUES(1,'BBB',19);
INSERT INTO USUARIO_REG VALUES(1,'JJJ',21);
SELECT * FROM USUARIO_REG;
